<%= form_for(partido) do |f| %>
  <% if partido.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(partido.errors.count, "error") %> prohibited this partido from being saved:</h2>
      <ul>
        <% partido.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>
  <div class="field">
    <%= f.label :equipo1 %>
    <%= render 'equipos/form' %>
  </div>
  <div class="field">
    <%= f.label :equipo2 %>
    <%= render 'equipos/form' %>
  </div>
  <div class="field">
    <%= f.label :resultado %>
    <%= f.number_field :resultado %>
  </div>
  <div class="field">
    <%= f.label :comentario %>
    <%= f.text_area :comentario %>
  </div>
  <div class="field">
    <%= f.label :fecha %>
    <%= f.date_select :fecha %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>